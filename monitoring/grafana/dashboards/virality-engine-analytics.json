{
  "dashboard": {
    "title": "Virality Engine - Analytics Dashboard",
    "tags": ["virality", "analytics"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Virality Probability Distribution",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_over_time(virality_probability[1h])",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "histogram", "show": true}
      },
      {
        "id": 2,
        "title": "Prediction Confidence Distribution",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_over_time(prediction_confidence[1h])",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "histogram", "show": true}
      },
      {
        "id": 3,
        "title": "Content Ingestion by Platform",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum by (platform) (content_ingested_total)",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 4,
        "title": "Content Ingestion by Type",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum by (content_type) (content_ingested_total)",
            "legendFormat": "{{content_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "pieType": "pie"
        }
      },
      {
        "id": 5,
        "title": "Average Virality Probability Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "avg_over_time(virality_probability[5m])",
            "legendFormat": "Average",
            "refId": "A"
          },
          {
            "expr": "max_over_time(virality_probability[5m])",
            "legendFormat": "Max",
            "refId": "B"
          },
          {
            "expr": "min_over_time(virality_probability[5m])",
            "legendFormat": "Min",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Probability", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true}
      },
      {
        "id": 6,
        "title": "Prediction Throughput",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(content_scored_total[5m])",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Predictions/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time", "show": true}
      },
      {
        "id": 7,
        "title": "End-to-End Latency Breakdown",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1.0, "color": "yellow"},
                {"value": 2.0, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  }
}

